                  </m:msup>
                </m:mrow>
              </m:math>
            </item>
            <item id="uid165">
              <m:math overflow="scroll">
                <m:mrow>
                  <m:mo form="prefix">sin</m:mo>
                  <m:msup>
                    <m:mn>320</m:mn>
                    <m:mo>∘</m:mo>
                  </m:msup>
                  <m:mo>=</m:mo>
                  <m:mo form="prefix">sin</m:mo>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:msup>
                      <m:mn>360</m:mn>
                      <m:mo>∘</m:mo>
                    </m:msup>
                    <m:mo>-</m:mo>
                    <m:msup>
                      <m:mn>40</m:mn>
                      <m:mo>∘</m:mo>
                    </m:msup>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mo>=</m:mo>
                  <m:mo>-</m:mo>
                  <m:mo form="prefix">sin</m:mo>
