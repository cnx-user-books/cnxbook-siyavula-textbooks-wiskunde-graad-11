										</m:mtd>
										<m:mtd columnalign="left">
											<m:msup>
												<m:mfenced separators="" open="(" close=")">
													<m:mfrac>
														<m:mn>5</m:mn>
														<m:mrow>
															<m:mfrac>
																<m:mn>1</m:mn>
																<m:mn>4</m:mn>
															</m:mfrac>
