		<exercise id="secfhsst_id364"><title> Meer Rasionele Eksponente </title>
			<problem id="probfhsst_id365">
				<para id="id76366">Vereenvoudig:</para>
				<equation id="id76372">
					<m:math overflow="scroll" mode="display">
						<m:msup>
							<m:mrow>
								<m:mo>(</m:mo>
								<m:mn>16</m:mn>
								<m:msup>
									<m:mi>x</m:mi>
									<m:mn>4</m:mn>
								</m:msup>
								<m:mo>)</m:mo>
							</m:mrow>
							<m:mfrac>
								<m:mn>3</m:mn>
								<m:mn>4</m:mn>
							</m:mfrac>
						</m:msup>
					</m:math>
				</equation>
			</problem>
			<solution id="solfhsst_id388">
				<list id="listfhsst_id388" list-type="enumerated" number-style="arabic" class="stepwise">
					<item>
						<label> Omskep die getal koëffisiënt na indeks-vorm met 'n prima basis </label>
						<equation id="id76426">
							<m:math overflow="scroll" mode="display">
								<m:mtable displaystyle="true">
									<m:mtr>
										<m:mtd/>
										<m:mtd>
											<m:mo>=</m:mo>
										</m:mtd>
										<m:mtd columnalign="left">
											<m:msup>
												<m:mrow>
													<m:mo>(</m:mo>
													<m:msup>
														<m:mn>2</m:mn>
														<m:mn>4</m:mn>
													</m:msup>
													<m:msup>
														<m:mi>x</m:mi>
														<m:mn>4</m:mn>
													</m:msup>
													<m:mo>)</m:mo>
												</m:mrow>
												<m:mfrac>
													<m:mn>3</m:mn>
													<m:mn>4</m:mn>
												</m:mfrac>
											</m:msup>
										</m:mtd>
									</m:mtr>
								</m:mtable>
							</m:math>
						</equation>
					</item>
					<item>
						<label> Gebruik eksponensiële wette </label>
						<equation id="id76479">
							<m:math overflow="scroll" mode="display">
								<m:mtable displaystyle="true">
									<m:mtr>
										<m:mtd/>
										<m:mtd>
											<m:mo>=</m:mo>
										</m:mtd>
										<m:mtd columnalign="left">
											<m:mrow>
												<m:msup>
													<m:mn>2</m:mn>
													<m:mrow>
														<m:mn>4</m:mn>
														<m:mo>×</m:mo>
