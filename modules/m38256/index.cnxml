												<m:mo>-</m:mo>
												<m:mn>1</m:mn>
											</m:mrow>
										</m:msup>
									</m:mrow>
								</m:mfrac>
							</m:mfenced>
							<m:mfrac>
								<m:mn>1</m:mn>
								<m:mn>2</m:mn>
							</m:mfrac>
						</m:msup>
					</m:math>
				</equation>
			</problem>
			<solution id="solfhsst_id224">
