              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mroot>
                    <m:mn>27</m:mn>
                    <m:mn>3</m:mn>
                  </m:mroot>
                  <m:mo>·</m:mo>
                  <m:mroot>
                    <m:mn>2</m:mn>
                    <m:mn>3</m:mn>
                  </m:mroot>
                  <m:mo>+</m:mo>
