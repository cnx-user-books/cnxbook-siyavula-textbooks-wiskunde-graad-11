                      <m:mrow>
                        <m:mi>K</m:mi>
                        <m:mi>I</m:mi>
                      </m:mrow>
                    </m:mfrac>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>
                          <m:mi>H</m:mi>
