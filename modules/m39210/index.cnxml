                      <m:mi>I</m:mi>
                    </m:mrow>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mi>J</m:mi>
                      <m:mi>K</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>K</m:mi>
                      <m:mi>I</m:mi>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd/>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mfrac>
                        <m:mn>5</m:mn>
                        <m:mn>3</m:mn>
                      </m:mfrac>
                      <m:mi>K</m:mi>
                      <m:mi>I</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>K</m:mi>
                      <m:mi>I</m:mi>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd/>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mfrac>
                        <m:mn>8</m:mn>
                        <m:mn>3</m:mn>
                      </m:mfrac>
                      <m:mi>K</m:mi>
                      <m:mi>I</m:mi>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>J</m:mi>
                        <m:mi>I</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>K</m:mi>
                        <m:mi>I</m:mi>
                      </m:mrow>
                    </m:mfrac>
                  </m:mtd>
                  <m:mtd>
