                      <m:mrow>
                        <m:mtext>gebied</m:mtext>
                        <m:mspace width="4.pt"/>
                        <m:mi>▵</m:mi>
                        <m:mtext>BDE</m:mtext>
                      </m:mrow>
                    </m:mfrac>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
