                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mrow>
                      <m:mo>∴</m:mo>
                      <m:mtext>DE</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mtext>deel</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mtext>AB</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mtext>en</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mtext>AC</m:mtext>
