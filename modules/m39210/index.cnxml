                            <m:mn>2</m:mn>
                          </m:mfrac>
                          <m:mi>A</m:mi>
                          <m:mi>D</m:mi>
                          <m:mo>·</m:mo>
                          <m:msub>
                            <m:mi>h</m:mi>
                            <m:mn>1</m:mn>
                          </m:msub>
                        </m:mrow>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mn>2</m:mn>
                          </m:mfrac>
                          <m:mi>D</m:mi>
                          <m:mi>B</m:mi>
                          <m:mo>·</m:mo>
                          <m:msub>
                            <m:mi>h</m:mi>
                            <m:mn>1</m:mn>
                          </m:msub>
                        </m:mrow>
                      </m:mfrac>
                      <m:mo>=</m:mo>
                      <m:mfrac>
                        <m:mrow>
                          <m:mi>A</m:mi>
                          <m:mi>D</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>D</m:mi>
                          <m:mi>B</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mfrac>
                      <m:mrow>
                        <m:mtext>gebied</m:mtext>
                        <m:mspace width="4.pt"/>
                        <m:mi>▵</m:mi>
                        <m:mtext>ADE</m:mtext>
                      </m:mrow>
                      <m:mrow>
                        <m:mtext>gebied</m:mtext>
                        <m:mspace width="4.pt"/>
                        <m:mi>▵</m:mi>
                        <m:mtext>CED</m:mtext>
                      </m:mrow>
                    </m:mfrac>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mn>2</m:mn>
                          </m:mfrac>
                          <m:mi>A</m:mi>
                          <m:mi>E</m:mi>
                          <m:mo>·</m:mo>
                          <m:msub>
                            <m:mi>h</m:mi>
                            <m:mn>2</m:mn>
                          </m:msub>
                        </m:mrow>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mn>2</m:mn>
                          </m:mfrac>
                          <m:mi>E</m:mi>
                          <m:mi>C</m:mi>
                          <m:mo>·</m:mo>
                          <m:msub>
                            <m:mi>h</m:mi>
                            <m:mn>2</m:mn>
                          </m:msub>
                        </m:mrow>
                      </m:mfrac>
                      <m:mo>=</m:mo>
                      <m:mfrac>
                        <m:mrow>
                          <m:mi>A</m:mi>
                          <m:mi>E</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>E</m:mi>
                          <m:mi>C</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mrow>
                      <m:mtext>maar</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mfrac>
                        <m:mrow>
                          <m:mi>A</m:mi>
                          <m:mi>D</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>D</m:mi>
                          <m:mi>B</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mrow>
                  </m:mtd>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>
                          <m:mi>A</m:mi>
                          <m:mi>E</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>E</m:mi>
                          <m:mi>C</m:mi>
                        </m:mrow>
                      </m:mfrac>
                      <m:mspace width="4.pt"/>
                      <m:mtext>(gegee)</m:mtext>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mrow>
                      <m:mo>∴</m:mo>
                      <m:mfrac>
