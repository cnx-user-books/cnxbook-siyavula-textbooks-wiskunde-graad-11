                    <m:mspace width="1.em"/>
                    <m:mi>x</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>11</m:mn>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
            </m:mtable>
          </m:math>
        </equation>
        
        </item> </list> </solution>
